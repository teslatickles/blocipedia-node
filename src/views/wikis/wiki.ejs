<!DOCTYPE html>
<html>

<head>
    <% include ../static/partials/head.ejs %>
</head>

<body>
    <main class="container">
        <% include ../static/partials/navbar.ejs %>
        <h1>Wikis</h1>

        <ul class="list-group">
            <!-- #1 -->
            <% wikis.forEach((wiki) => { %>
            <!-- #2 -->
            <li class="list-group-item">
                <a href="/wikis/<%= wiki.id %>">
                    <%= wiki.title %> </a>
            </li>
            <% }) %>
        </ul>
    </main>
</body>

</html>