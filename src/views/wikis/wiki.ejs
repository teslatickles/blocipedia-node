<!DOCTYPE html>
<html>

<head>
    <% include ../static/partials/head.ejs %>
</head>

<body>
    <main class="container">
        <% include ../static/partials/navbar.ejs %>
        <h1>Wikis</h1>


        <a class="btn btn-warning" href="/wikis/new">New Wiki</a>

        <ul class="list-group">


            <% wikis.forEach((wiki) => { %>

            <% if (wiki.private === false || wiki.private === true) { %>
            <!-- #2 -->
            <li class="list-group-item">
                <a href="/wikis/<%= wiki.id %>">
                    <%= wiki.title %> </a>
            </li>
            <% } %>
            <% }) %>
        </ul>
    </main>
</body>

</html>